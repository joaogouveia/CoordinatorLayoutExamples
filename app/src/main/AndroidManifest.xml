<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.concrete.coordinatorlayoutexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.activity.Coordinator1Activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".ui.activity.Coordinator2Activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity
            android:name=".ui.activity.Coordinator3Activity"
            android:label="@string/trem_bala"
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity
            android:name=".ui.activity.Coordinator4Activity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar"/>

        <activity
            android:name=".ui.activity.Coordinator5Activity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>